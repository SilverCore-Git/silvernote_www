{
  "extends": "./.nuxt/tsconfig.json",
  "compilerOptions": {
    "rootDir": ".",
    "lib": ["ES2023", "DOM", "DOM.Iterable"],
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noFallthroughCasesInSwitch": true
  },
  "include": [
    ".nuxt/nuxt.d.ts",
    "src/**/*",
    "app.vue"
  ]
}
